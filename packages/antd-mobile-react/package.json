{
  "name": "@trionesdev/antd-mobile-react",
  "version": "0.0.2-beta.2",
  "description": "antd mobile react ",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "start-dev": "cross-env API_PARSER=true npm run dev",
    "dev": "dumi dev",
    "build": "father build",
    "build:watch": "father dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "prepare": " dumi setup",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build",
    "publish": "npm publish --registry=https://registry.npmjs.org/",
    "publishOnly": "npm publish --ignore-scripts --registry=https://registry.npmjs.org/"
  },
  "authors": [
    "fengxiaotx@163.com"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "classnames": "^2.5.1",
    "lodash-es": "^4.17.21",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "staged-components": "^1.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@trionesdev/antd-mobile-base-react": "workspace:*",
    "@trionesdev/antd-mobile-icons-react": "workspace:*",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "@umijs/lint": "^4.0.0",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "dumi": "2.4.21",
    "dumi-theme-mobile": "^2.3.5",
    "eslint": "^8.23.0",
    "father": "^4.6.9",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "staged-components": "^1.1.3",
    "stylelint": "^16.12.0"
  },
  "dependencies": {
    "@trionesdev/antd-mobile-base-react": "^0.0.2-beta.2",
    "ahooks": "^3.8.4",
    "crypto-js": "^4.2.0",
    "rc-field-form": "^2.7.0",
    "rc-util": "^5.44.3",
    "runes2": "^1.1.4"
  },
  "optionalDependencies": {
    "@trionesdev/antd-mobile-base-react": "workspace:*",
